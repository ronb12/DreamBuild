# DreamBuild Environment Variables
# Copy this file to .env.local and fill in your API keys

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456789

# AI Service API Keys (Optional - Free tiers available)
VITE_GROQ_API_KEY=your_groq_api_key
VITE_HUGGINGFACE_TOKEN=your_huggingface_token
VITE_TOGETHER_API_KEY=your_together_api_key

# Note: Ollama runs locally and doesn't require an API key
